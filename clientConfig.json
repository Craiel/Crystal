{
  "Name": "Crystal",
  "Data": [
    {
      "Directory": "data\\content\\",
      "FilterStrings": [
        "*.xlsx"
      ]
    }
  ],
  "Templates": [
    {
      "Directory": "data\\templates\\",
      "FilterStrings": [
        "*.html"
      ]
    }
  ],
  "Sources": [
  	{
      "Directory": "src\\system\\",
      "FilterStrings": [ "system.js", "loader.js" ]
    },
    {
      "Directory": "src\\client\\",
      "FilterStrings": [ "init.js" ]
    },
    {
      "Directory": "src\\system\\",
      "FilterStrings": [
        "*.js"
      ],
      "Option": 1
    },
    {
      "Directory": "src\\common\\",
      "FilterStrings": [
        "*.js"
      ],
      "Option": 1
    },
    {
      "Directory": "src\\client\\",
      "FilterStrings": [
        "*.js"
      ],
      "Option": 1
    },
    {
      "Directory": "src\\data\\",
      "FilterStrings": [
        "*.js"
      ],
      "Option": 1
    },
    {
      "Directory": "src\\client\\",
      "FilterStrings": [ "main.js" ]
    },
  ],
  "StyleSheets": [
    {
      "Directory": "data\\css\\",
      "FilterStrings": [ "*.css" ],
      "Option": 1
    }
  ],
  "Contents": [
    {
      "Directory": "www\\",
      "FilterStrings": [
        "*"
      ]
    },
    {
      "Directory": "data\\img",
      "FilterStrings": [
        "*.png"
      ],
      "Option": 1
    }
  ],
  "SourceTarget": "bin\\client\\crystal.js",
  "TemplateTarget": "src\\data\\generated\\templates.js",
  "DataTarget": "src\\data\\generated\\data.js",
  "StyleSheetTarget": "bin\\client\\crystal.css",
  "ContentTarget": "bin\\client\\"
}
